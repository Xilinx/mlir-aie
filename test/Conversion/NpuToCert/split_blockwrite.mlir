// Test the SplitNpuBlockWriteOpPattern executed as part of -aie-npu-to-cert.
// A single large aiex.npu.blockwrite with a 2000xi32 global (8000 bytes)
// should be split into two blockwrites with two new 1000xi32 globals.
// After lowering to cert, we expect two UC DMA BD ops referencing the split
// globals (original global erased if unused).
//
// RUN: aie-opt --aie-npu-to-cert %s | FileCheck %s
//
// CHECK: memref.global "private" constant @data_split_0 : memref<1000xi32> = dense<"0x123456789ABCDEF
// CHECK: memref.global "private" constant @data_split_1 : memref<1000xi32> = dense<"0xDEADBEEFDEADBEEF
// CHECK: aiex.cert.uc_dma_bd @data_split_0, 4096, 1000, false
// CHECK: aiex.cert.uc_dma_bd @data_split_1, 8096, 1000, false

module {
  aie.device(npu2) {
    // Original large global (exact boundary == 8000 bytes) triggers split.
    memref.global "private" constant @data : memref<2000xi32> = dense<"">
    aie.runtime_sequence @configure() {
      %g = memref.get_global @data : memref<2000xi32>
      aiex.npu.blockwrite(%g) {address = 4096 : ui32} : memref<2000xi32>
      aie.end
    }
  }
}
